<% admin_layout "full-width" %>

<% admin_breadcrumb(t("spree.settings")) %>
<% admin_breadcrumb link_to(plural_resource_name(Spree::ProductFeed), spree.admin_product_feeds_path) %>
<% admin_breadcrumb t("spree.new_product_feed") %>

<%= render partial: "spree/shared/error_messages", locals: { target: @product_feed } %>

<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<%= form_for [:admin, @product_feed], html: { class: "new_product_feed product-feed-form-container mt-0" } do |f| %>
  <%= render partial: "form", locals: { f: f, action: "new_product_feed" } %>
  <%= render partial: "spree/admin/shared/new_resource_links", locals: { collection_url: admin_stores_url } %>
<% end %>
